        android:id="@+id/Description"
        android:layout_width="206dp"
        android:layout_height="81dp"
        android:background="@null"
        android:ems="10"
        android:gravity="start|fill_horizontal|top"
        android:hint="                                 Description"
        android:inputType="textMultiLine"
        app:layout_constraintBottom_toBottomOf="@+id/Description_holder"
        app:layout_constraintEnd_toEndOf="@+id/Description_holder"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="@+id/Description_holder"
        app:layout_constraintTop_toTopOf="@+id/Description_holder"
