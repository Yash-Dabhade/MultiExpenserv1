    <EditText
        android:id="@+id/Month"
        android:layout_width="53dp"
        android:layout_height="39dp"
        android:background="@null"
        android:ems="10"
        android:hint="Month"
        android:inputType="numberSigned"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/Month_holder"
        app:layout_constraintEnd_toEndOf="@+id/Month_holder"
        app:layout_constraintHorizontal_bias="0.652"
        app:layout_constraintStart_toStartOf="@+id/Month_holder"
        app:layout_constraintTop_toTopOf="@+id/Month_holder" />

    <EditText
        android:id="@+id/Year"
        android:layout_width="55dp"
        android:layout_height="30dp"
        android:background="@null"
