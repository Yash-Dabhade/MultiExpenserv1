        android:hint="Year"
        android:inputType="numberSigned"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/Year_holder"
        app:layout_constraintEnd_toEndOf="@+id/Year_holder"
        app:layout_constraintHorizontal_bias="0.714"
        app:layout_constraintStart_toStartOf="@+id/Year_holder"
        app:layout_constraintTop_toTopOf="@+id/Year_holder"
        app:layout_constraintVertical_bias="0.49" />

