        android:id="@+id/Title"
        android:layout_width="206dp"
        android:layout_height="41dp"
        android:background="@null"
        android:ems="10"
        android:hint="Title"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="@+id/Title_holder"
        app:layout_constraintEnd_toEndOf="@+id/Title_holder"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="@+id/Title_holder"
        app:layout_constraintTop_toBottomOf="@+id/imageView22"
        app:layout_constraintVertical_bias="0.583" />

    <EditText
        android:id="@+id/Amount"
        android:layout_width="188dp"
        android:layout_height="39dp"
        android:background="@null"
        android:ems="10"
        android:hint="Amount"
        android:inputType="numberSigned"
        app:layout_constraintBottom_toBottomOf="@+id/Amount_holder"
        app:layout_constraintEnd_toEndOf="@+id/Amount_holder"
        app:layout_constraintHorizontal_bias="0.349"
        app:layout_constraintStart_toStartOf="@+id/Amount_holder"
        app:layout_constraintTop_toBottomOf="@+id/Title_holder" />

    <EditText
        android:id="@+id/Day"
        android:layout_width="40dp"
        android:layout_height="38dp"
